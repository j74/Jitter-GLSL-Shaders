<jittershader name="Exposure"> 
	<param name="amount" type="float" default="1." />
	<param name="color" type="vec4" default="0.9 0.6 0.8 1.0" />
	<language name="glsl" version="1.0"> 
		<bind param="tex0" program="fp" />
		<bind param="amount" program="fp" />
		<bind param="color" program="fp" />
		<program name="vp" type="vertex" source="v001.tintlight.vp.glsl" />
		<program name="fp" type="fragment" source="v001.tintlight.fp.glsl" /> 
	</language> 
</jittershader> 
