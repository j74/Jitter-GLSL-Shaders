<jittershader name="Exposure"> 
	<param name="tex0" type="int" default="0" />
	<param name="scale" type="float" default="1." />
	<language name="glsl" version="1.0"> 
		<bind param="tex0" program="fp" />
		<bind param="scale" program="fp" />
		<program name="vp" type="vertex" source="v001.pixelate.vp.glsl" />
		<program name="fp" type="fragment" source="v001.pixelate.fp.glsl" /> 
	</language> 
</jittershader> 
