<jittershader name="2 Pass Blur"> 
	<param name="width" type="vec2" default="0. 0." />
	<param name="offset" type="vec2" default="0. 0." />
	<language name="glsl" version="1.0"> 
		<bind param="tex0" program="fp" />
		<bind param="width" program="vp" />
		<bind param="offset" program="vp" />
		<program name="vp" type="vertex" source="v001.shred.vp.glsl" />
		<program name="fp" type="fragment" source="v001.shred.fp.glsl" /> 
	</language> 
</jittershader> 
