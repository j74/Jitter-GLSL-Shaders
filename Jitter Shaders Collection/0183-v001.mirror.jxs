<jittershader name="2 Pass Blur"> 
	<param name="origin" type="vec2" default="0. 0." />
	<language name="glsl" version="1.0"> 
		<bind param="tex0" program="fp" />
		<bind param="vertical" program="fp" />
		<bind param="origin" program="fp" />
		<program name="vp" type="vertex" source="v001.mirror.vp.glsl" />
		<program name="fp" type="fragment" source="v001.mirror.fp.glsl" /> 
	</language> 
</jittershader> 
