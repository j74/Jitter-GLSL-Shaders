<jittershader name="AB Additive"> 
	<param name="thresh" type="float" default="0.4" />
	<param name="tex0" type="int" default="0" /> 
	<language name="glsl" version="1.0"> 
		<bind param="tex0" program="fp" />
		<bind param="thresh" program="fp" />
		<program name="vp" type="vertex" source="v001.test1.vp.glsl" />
		<program name="fp" type="fragment" source="v001.test1.fp.glsl" /> 
	</language> 
</jittershader> 
